<% for(var i=0; i< employees.length; i++) {%>
<tr>
    <td><%=i+1%></td>
    <td><%=employees[i].name%></td>
    <td><%=employees[i].email%></td>
    <td><%=employees[i].phoneNumber%></td>
    <td> 
        <a href="/update_employee?id=<%=employees[i]._id%>" class="btn border-shadow update">
            <span class="text-update"><i class="fas fa-pencil-alt"></i> </span>
        </a>
        <a class="btn border-shadow delete" data-id=<%=employees[i]._id%>>
            <span class="text-del"><i class="fas fa-trash"></i> </span>
        </a>
    </td>
</tr>
<% }%>